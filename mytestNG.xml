    <listeners>
        <listener class-name="com.syam.test.utils.report.MyTestLisiner"></listener>
    </listeners>
    <test name="this is regression" >

        <classes>
            <class name="com.syam.test.orders.MyOrdersTestClass"></class>
            <class name="com.syam.test.users.MyUsersTestClass"></class>
            <class name="com.syam.test.OrdersNUsers.OrdersNUsersTest"></class>
            <class name="com.syam.test.restfill.RestFullServicesTest"></class>
        </classes>
    </test>

    <test name="this is smpke">
